{
  "manifest_version": 2,
  "name": "Soma Academy",
  "description": "Ignores X-Frame-Options to allow iFrames in Soma Academy",
  "version": "1.0.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "icons": {
    "128": "icons/128.png"
  }
}
